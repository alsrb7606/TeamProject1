<GridPane hgap="5.0" prefHeight="103.0" prefWidth="355.0" vgap="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.PutController">
   <padding>
