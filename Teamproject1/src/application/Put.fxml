   				<Button fx:id="putInfo" onAction="#PutOk" prefHeight="30.0" prefWidth="70.0" text="입력" />
   				<Button fx:id="backroot" onAction="#backRoot" prefHeight="30.0" prefWidth="70.0" text="화면 복귀" />
