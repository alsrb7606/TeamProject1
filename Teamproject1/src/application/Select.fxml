<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="400.0" prefWidth="400.0"  xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SelectController">
   <children>
   <VBox>
   	  <HBox>
   	  		<Label text="자리 번호"/>
   	  		<TextField fx:id="LocationNum"  prefHeight="57.0" prefWidth="211.0" />
   	  </HBox>	
   	  <HBox>
   	  		<Label text="차번호"/>
   	  		<TextField fx:id="CarNum"  prefHeight="57.0" prefWidth="211.0" />
   	  </HBox>
   	  <HBox>
   	  		<Label text="주차시간"/>
   	  		<TextField fx:id="CarTime"  prefHeight="57.0" prefWidth="211.0" />	
   	  </HBox>
   </VBox>		
      <HBox alignment="CENTER" layoutY="269.0" prefHeight="150.0" prefWidth="400.0">
         <children>
 			        
            <Button fx:id="putIn" mnemonicParsing="false" onAction="#PutInCar" prefHeight="150.0" prefWidth="200.0" text="입차" />
            <Button fx:id="putOut" mnemonicParsing="false" onAction="#PutOutCar" prefHeight="150.0" prefWidth="200.0" text="출차" />
         </children>
      </HBox>
      
   </children>
</AnchorPane>
